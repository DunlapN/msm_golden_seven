<h1>List of Movies</h1>
<table>
  <tr>
    <th>Title</th>
    <th>Year</th>
    <th>Duration</th>
    <th>Description</th>
    <th>Image</th>
  </tr>
  <% @list_of_movies.each do |movie| %>
  <tr>
    <td>
      <%= movie.title %>
    </td>
    <td>
      <%= movie.year %>
    </td>
    <td>
      <%= movie.duration %>
    </td>
    <td>
      <%= movie.description %>
    </td>
    <td>
      <%= movie.image_url %>
    </td>
    <td>
      <a href="http://localhost:3000/delete_movies/<%= movie.id %>">
        Delete
      </a>
    </td>
    <td>
    <a href="http://localhost:3000/movies/<%= movie.id %>">Show</a>
  </td>
  <td>
    <a href="http://localhost:3000/movies/<%= movie.id %>/edit_form">Edit</a>
  </td>
  </tr>
  <% end %>

</table>
