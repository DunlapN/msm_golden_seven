<h1>Edit a new actor</h1>
<form  action="/actors/update_row/<%=@actor.id %>">
  <div >
    <label for="actor_name"> actor Name</label>
    <input id="actor_name" name="name" type = "string" value = "<%=  @actor.name%>">
  </div>
  <div >
    <label for="actor_dob"> actor DOB</label>
    <input id="actor_dob" name="dob" type = "string" value = "<%=  @actor.dob%>">
  </div>
  <div >
    <label for="actor_bio"> actor Bio</label>
    <input id="actor_bio" name="bio" type = "text" value = "<%=  @actor.bio%>">
  </div>
  <div >
    <label for="actor_image"> actor Image</label>
    <input id="actor_image" name="image_url" type = "string" value = "<%=  @actor.image_url%>">
  </div>
  <div>
      <button>Edit actor </button>
  </div>

</form>
