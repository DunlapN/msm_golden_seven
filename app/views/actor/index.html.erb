<h1>List of Actors</h1>
<table>
  <tr>
    <th>Name</th>
    <th>Bio</th>
    <th>Date of Birth</th>
    <th>Image</th>
  </tr>
  <% @list_of_actors.each do |actor| %>
  <tr>
    <td>
      <%= actor.name %>
    </td>
    <td>
      <%= actor.bio %>
    </td>
    <td>
      <%= actor.dob %>
    </td>
    <td>
      <%= actor.image_url %>
    </td>
    <td>
      <a href="http://localhost:3000/delete_actors/<%= actor.id %>">
        Delete
      </a>
    </td>
    <td>
    <a href="http://localhost:3000/actors/<%= actor.id %>">Show</a>
  </td>
  <td>
    <a href="http://localhost:3000/actors/<%= actor.id %>/edit_form">Edit</a>
  </td>
  </tr>
  <% end %>

</table>
